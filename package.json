{
  "name": "io-landing",
  "vendor": "vtex",
  "title": "VTEX IO landing page",
  "description": "VTEX IO landing page",
  "version": "2.0.0",
  "micro": {
    "plugins": [
      "@vtex/micro-core",
      "io-landing",
      "@vtex/micro-css",
      "@vtex/micro-react",
      "@vtex/micro-assets",
      "@vtex/micro-react-router",
      "@vtex/micro-react-intl"
    ]
  },
  "scripts": {
    "build": "yarn run micro build && yarn run micro assemble",
    "watch": "yarn run micro link",
    "clean": "rm -r dist",
    "prepublish": "yarn build",
    "start": "yarn micro serve -p ${PORT}"
  },
  "main": "./dist/build/cjs/index.js",
  "module": "./dist/build/es6/components/index.js",
  "browser": "./components/index.ts",
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@vtex/micro-assets": "^1.1.1-alpha.9",
    "@vtex/micro-core": "^1.1.1-alpha.9",
    "@vtex/micro-css": "^1.1.1-alpha.9",
    "@vtex/micro-react": "^1.1.1-alpha.9",
    "@vtex/micro-react-intl": "^1.1.1-alpha.9",
    "@vtex/micro-react-router": "^1.1.1-alpha.9",
    "@vtex/styleguide": "9.119.0-micro.4",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-transform-remove-imports": "^1.3.2",
    "file-loader": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.8.5",
    "webpack-blocks": "^2.1.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.7",
    "@types/loadable__component": "^5.10.0",
    "@types/react": "^16.9.35",
    "@types/webpack-blocks": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "@vtex/micro-cli": "^1.1.1-alpha.9",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "typescript": "^3.9.3"
  },
  "engines": {
    "yarn": "2.x"
  }
}
